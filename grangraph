var xLimit = 5;
                var chart = nv.models.GranGraph2(xLimit);
                var DataSet = [];

                var newData = {
                    "ID":12,
                    "Name":"Amostra 1",
                    "values": [[0, 0.1],[1, 9.8],[2, 79.4],[3, 10.7]],
                    "Histogram": {
